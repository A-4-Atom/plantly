{
  "expo": {
    "name": "Plantly",
    "slug": "plantly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/plantly.png",
      "resizeMode": "contain",
      "backgroundColor": "#eeeeee"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vikascsgo.plantly"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/caveat/400Regular/Caveat_400Regular.ttf"
          ]
        }
      ],
      ["expo-quick-actions", {
        "androidIcons": {
          "leaf": {
            "foregroundImage": "./assets/leaf.png",
            "backgroundColor": "#EEEEEE"
          }
        }
      }]
    ],
    "scheme": "plantly",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd28870b-6492-45b5-9e36-71d98ddb8494"
      }
    }
  }
}
